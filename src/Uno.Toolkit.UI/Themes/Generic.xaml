<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
					xmlns:toolkit="using:Uno.Toolkit.UI.Controls"
					xmlns:ios="http://uno.ui/ios"
					xmlns:win="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:android="http://uno.ui/android"
					xmlns:netstdref="http://uno.ui/netstdref"
					mc:Ignorable="android ios netstdref">

	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="/Controls/AutoLayout/AutoLayout.xaml" />
	</ResourceDictionary.MergedDictionaries>
	
	<!-- TabBar -->
	<Style TargetType="toolkit:TabBar"
		   BasedOn="{StaticResource DefaultTabBarStyle}" />

	<!-- TabBarItem -->
	<Style TargetType="toolkit:TabBarItem"
		   BasedOn="{StaticResource DefaultTabBarItemStyle}" />
</ResourceDictionary>
